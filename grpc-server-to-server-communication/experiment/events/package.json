{
  "name": "jscoe-90-grpc-experiment-events",
  "version": "1.0.0",
  "description": "A simple solution for creating and managing events",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "tsc && node ./dist",
    "start:docker": "npm-run-all --parallel build:watch docker",
    "docker": "nodemon --inspect=0.0.0.0:9229 ./dist",
    "migration": "node ./dist/db/migrations",
    "lint": "tslint src/**/*.ts",
    "format": "tslint src/**/*.ts --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "logs/*",
      ".vscode/*",
      "node_modules/*"
    ],
    "delay": "2500"
  },
  "author": "Valentine Oleka <valentine.oleka@moduscreate.com>",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.5.0",
    "@grpc/proto-loader": "^0.6.9",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "mysql2": "^2.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/mysql": "^2.15.20",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4"
  }
}
