{
  "name": "minimal-react-webpack-babel-eslint-prettier-setup",
  "version": "1.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "yarn run relay && webpack serve --mode development",
    "build": "webpack --mode production",
    "lint": "eslint ./src --cache && echo \"No linting errors\"",
    "fakeapi": "json-server ./src/api/db.json",
    "lint:fix": "eslint ./src --fix",
    "relay": "yarn run relay-compiler --schema schema.graphql --src ./src/ --watchman false $@"
  },
